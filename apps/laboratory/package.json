    "@sentry/react": "7.92.0",
    "@tanstack/react-query": "5.17.19",
    "@web3modal/ethers": "4.1.1",
    "@web3modal/siwe": "4.1.1",
    "@web3modal/wagmi": "4.1.1",
    "ethers": "6.9.0",
    "framer-motion": "10.17.9",
    "next": "14.0.4",
    "next-auth": "4.24.5",
    "react-icons": "4.12.0",
